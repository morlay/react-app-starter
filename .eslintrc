{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "react/jsx-no-bind": 0,
    "react/prop-types": [
      2,
      {
        "ignore": [
          "children"
        ]
      }
    ]
  }
}
