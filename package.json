{
  "private": true,
  "config": {
    "MOCHA_OPTS": "-R dot --timeout 20000 --compilers .:babel-core/register {modules,src}/{,**/}**/__tests__/*.spec.js"
  },
  "scripts": {
    "lint": "eslint ./ --fix",
    "build": "rm -rf public && webpack",
    "mocha": "BABEL_DISABLE_CACHE=1 mocha $npm_package_config_MOCHA_OPTS",
    "release": "NODE_ENV=production npm run build",
    "dev": "SOURCE_MAP=1 webpack-browser-sync --hot --historyApiFallback"
  },
  "devDependencies": {
    "@morlay/babel-preset": "1.0.x",
    "@morlay/eslint-config": "1.0.x",
    "@morlay/tests-react": "1.0.x",
    "autoprefixer-stylus": "0.9.x",
    "babel-loader": "6.2.x",
    "babel-plugin-module-alias": "1.6.x",
    "babel-plugin-system-import-transformer": "2.2.x",
    "babel-plugin-transform-es2015-modules-commonjs": "6.11.x",
    "babel-plugin-webpack-loaders-inline-exports": "0.2.x",
    "babel-preset-es2015-webpack": "6.4.x",
    "babel-preset-react": "6.11.x",
    "config": "1.21.x",
    "css-loader": "0.23.x",
    "eslint-import-resolver-babel-module-alias": "1.4.x",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "0.9.x",
    "html-webpack-plugin": "2.22.x",
    "react-hot-loader": "3.0.0-beta.1",
    "simplify-svg": "0.1.x",
    "style-loader": "0.13.x",
    "stylus": "0.54.x",
    "stylus-loader": "2.1.x",
    "svg2jsx": "0.1.x",
    "url-loader": "0.5.x",
    "webpack": "2.1.0-beta.20",
    "webpack-browser-sync": "0.1.x"
  },
  "dependencies": {
    "babel-runtime": "6.11.x",
    "classnames": "2.2.x",
    "normalize.css": "4.2.x",
    "react": "15.3.x",
    "react-dom": "15.3.x"
  }
}
